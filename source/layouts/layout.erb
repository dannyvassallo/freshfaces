<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= partial "partials/meta-seo" %>    
    <!-- Stylesheets -->
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,200,200italic,300,300italic,900italic,900,700italic,700,600italic,600,400italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <%= stylesheet_link_tag "all" %>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
    <div id="tearleft"></div>
    <div id="tearright"></div>
    <div id="overlay1"></div>
    <div id="overlay2"></div>
    <!-- Javascripts -->
    <%= javascript_include_tag  "all" %>
  </body>
</html>
